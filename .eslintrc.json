{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": "error",
        "import/no-named-as-default": 0,
        "no-param-reassign": 0,
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "prefer-arrow-callback": "off"
    }
}
